{
  "name": "projectname",
  "version": "0.0.1",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/src/index.js",
    "clear:logs": "node utilities/clearLogs.js",
    "build": "npx tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "lint:sonar": "eslint src --ext .ts --format json -o eslint-report.json",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "format:lint": "npm run format && npm run lint:fix",
    "sonar": "export $(grep -v '^#' .env.sonar | xargs) && sonar-scanner",
    "sonar:local": "docker run --rm -v \"$(pwd):/usr/src\" -w /usr/src sonarsource/sonar-scanner-cli",
    "sonar:up": "docker compose -f docker-compose.sonar.yml up -d",
    "sonar:down": "docker compose -f docker-compose.sonar.yml down",
    "quality:check": "npm run test:ci && npm run lint:sonar && npm run sonar"
  },
  "author": "Joneivison13",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.5.2",
    "amazon-cognito-identity-js": "^6.3.15",
    "amqplib": "^0.10.9",
    "aws-sdk": "^2.1692.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "esbuild": "^0.19.7",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2",
    "openid-client": "^6.8.0",
    "stompit": "^1.0.0",
    "tsoa": "^6.2.0",
    "uuid": "^13.0.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@jest/globals": "^30.1.2",
    "@types/amqplib": "^0.10.7",
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.2",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/openid-client": "^3.1.6",
    "@types/stompit": "^0.26.6",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-sonarjs": "^0.21.0",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "prisma": "^5.5.2",
    "sonarqube-scanner": "^3.3.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.1.1",
    "typescript": "^5.2.2"
  }
